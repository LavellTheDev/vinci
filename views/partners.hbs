      <!-- - - - - - - - - SafetyDNA VR ---------------->
        <section class="mb-4 panel background fourth">
          <section class="text-primary body-font">
            <h1 class="text-primary text-center sm:text-4xl text-3xl mt-10 -mb-10">SafetyDNA VR</h1>
            <section class="text-gray-700 body-font">
               <div class="container mx-auto flex px-5 py-24 md:flex-row flex-col items-center">
                 <div class="lg:max-w-lg lg:w-full md:w-1/2 w-5/6 mb-10 md:mb-0 mt-4">
